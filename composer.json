{
    "name": "hiqdev/omnipay-bitpay",
    "type": "library",
    "description": "BitPay driver for Omnipay PHP payment libray",
    "keywords": [
        "omnipay",
        "bitpay",
        "php",
        "merchant",
        "payment"
    ],
    "homepage": "https://github.com/hiqdev/omnipay-bitpay",
    "license": "MIT",
    "support": {
    },
    "authors": [
    ],
    "require": {
        "omnipay/common": "^3.0",
        "bitpay/sdk": "6.x"
    },
    "require-dev": {
        "omnipay/tests": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\BitPay\\": "src",
            "Omnipay\\BitPay\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
